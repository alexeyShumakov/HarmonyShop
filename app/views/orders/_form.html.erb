<%= simple_form_for([@user,@order]) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :pay_type %>
    <%= f.input :address %>
    <%= f.input :phone %>
    <%= f.input :delivery_type %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
